{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-transform-runtime",{"corejs": 2}],
    "@babel/plugin-transform-object-assign",
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
